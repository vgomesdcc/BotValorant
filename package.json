{
  "name": "vavabot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --root=src",
    "dev": "tsx src/main.ts",
    "devOld": "ts-node-esm src/main.ts",
    "test": "ts-node-esm src/main_test.ts",
    "start": "node build/main.js",
    "watch": "nodemon --exec ts-node-esm src/main.ts"
  },
  "dependencies": {
    "@discordx/importer": "1.1.10",
    "@discordx/utilities": "5.1.0",
    "axios": "^1.3.4",
    "cron": "2.1.0",
    "discord.js": "14.10.2",
    "discordx": "11.4.0",
    "dotenv": "16.0.3",
    "emoji-regex": "10.2.1",
    "firebase": "9.14.0"
  },
  "devDependencies": {
    "@types/cron": "2.0.0",
    "@types/node": "18.11.9",
    "nodemon": "2.0.20",
    "prettier": "2.7.1",
    "ts-node": "10.9.1",
    "tsx": "^4.19.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
